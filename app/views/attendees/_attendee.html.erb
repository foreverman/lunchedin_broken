<div class="attendee" id="attendee_<%= attendee.id %>">
	<dl>
		<dt>
			<span><%= attendee.name %></span>
			<span style="color: #cccccc; margin: 0 8px 0 8px;">|</span>
			<span style="color: #999999"><%= attendee.email %></span>
		</dt>
		<dd><%= link_to "uninvite", attendee_path(attendee), :method => :delete, remote: true %></dd>
	</dl>
</div>
